package Practica2;

public class ejemplo3 {

  public int iterativo(int c){
    int total=1;
    do{
      total =total*c;
      c--;
    }while (c>=1);
    return total;
  }

public int recursivo (int num){
  if (num==0){
    return 1;
  }
  else{
    return num*recursivo(num-1);
  }
}
